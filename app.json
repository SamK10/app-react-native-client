{
  "expo": {
    "sdkVersion": "37.0.0",
    "name": "MyAPP",
    "slug": "MyAPP",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.myapp.myapp",
      "enableDangerousExperimentalLeanBuilds": true
    },
    "ios": {
      "supportsTablet": true
    },
    "description": ""
  }
}
